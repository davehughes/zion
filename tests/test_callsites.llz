module _
# test: pass

type int has {
	raw __int__
}

fn __lt__(lhs int, rhs __int__) __bool__ {
	return call __lt__(lhs.raw, rhs)
}

fn myabs(x int) int {
	const v = call __lt__(x, 0)
	if v {
		# for now the minus operator/function needs to be binary
		return call int(call __negative__(x.raw))
	} else {
		return x
	}
}

fn main() {
	call myabs(call int(call __negative__(3)))
}
